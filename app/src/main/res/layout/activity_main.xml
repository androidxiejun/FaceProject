    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.xj.faceandidcardproject.view.MainActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <SurfaceView
            android:id="@+id/gatemodule_preSurfaceView"
