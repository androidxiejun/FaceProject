        <SurfaceView
            android:id="@+id/gatemodule_rectSurfaceView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerInParent="true" />

    </RelativeLayout>
</android.support.constraint.ConstraintLayout>
